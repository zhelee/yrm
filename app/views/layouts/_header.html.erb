<% if flash[:notice] %>
  <div class="alert">
    <button type="button" class="close" data-dismiss="alert">×</button>
    <strong>Warning!</strong> <%= notice %>
  </div>
<% end %>
<% if flash[:alert] %>
  <div class="alert alert-error">
    <button type="button" class="close" data-dismiss="alert">×</button>
    <strong>Warning!</strong> <%= alert %>
  </div>
<% end %>
